{
  "name": "sofie.fitness-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "debug": "nodemon --inspect dist/index.js",
    "generate": "DEBUG=1 graphql-codegen --config codegen.yml",
    "studio": "prisma studio --port 5000",
    "dbpush": "prisma db push",
    "migrate": "prisma migrate dev",
    "heroku-postbuild": "if [ $NODE_ENV == sandbox ] && npm run generate && npm run build || prisma migrate deploy && npm run generate && npm run build"
  },
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/merge": "^8.1.2",
    "@paljs/plugins": "^4.0.8",
    "@prisma/client": "^3.2.1",
    "apollo-server-express": "^3.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.12.0",
    "getstream": "^7.2.11",
    "graphql-config": "^4.0.2",
    "graphql-middleware": "^6.1.8",
    "graphql-parse-resolve-info": "^4.12.0",
    "graphql-type-json": "^0.3.2",
    "joi": "^17.4.2",
    "node-fetch": "^2.6.2",
    "stream-chat": "^4.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/schema-ast": "^2.2.0",
    "@graphql-codegen/typescript": "2.2.3",
    "@graphql-codegen/typescript-document-nodes": "2.1.5",
    "@graphql-codegen/typescript-resolvers": "^2.3.1",
    "@types/node": "^16.10.9",
    "@types/node-fetch": "^2.5.12",
    "@types/uuid": "^8.3.1",
    "concurrently": "^6.3.0",
    "graphql": "^15.6.1",
    "nodemon": "^2.0.13",
    "prisma": "^3.2.1",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
