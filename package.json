{
  "name": "sofie.fitness-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "debug": "nodemon --inspect dist/index.js",
    "generate": "graphql-codegen --config codegen.yml",
    "studio": "prisma studio --port 5000",
    "dbpush": "prisma db push",
    "postinstall": "prisma db push --accept-data-loss && npm run generate && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/merge": "^8.1.2",
    "@paljs/plugins": "^4.0.8",
    "@prisma/client": "^3.0.1",
    "apollo-server-express": "^2.25.2",
    "dotenv": "^10.0.0",
    "firebase-admin": "^9.10.0",
    "getstream": "^7.2.10",
    "graphql-config": "^3.3.0",
    "graphql-middleware": "^6.0.10",
    "graphql-parse-resolve-info": "^4.12.0",
    "graphql-type-json": "^0.3.2",
    "joi": "^17.4.0",
    "node-fetch": "^2.6.1",
    "stream-chat": "^3.13.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.6",
    "@graphql-codegen/schema-ast": "^1.18.3",
    "@graphql-codegen/typescript": "1.22.4",
    "@graphql-codegen/typescript-document-nodes": "1.17.15",
    "@graphql-codegen/typescript-resolvers": "^1.19.5",
    "@types/node": "^16.0.0",
    "@types/node-fetch": "^2.5.10",
    "@types/uuid": "^8.3.1",
    "concurrently": "^6.2.0",
    "graphql": "^15.5.1",
    "nodemon": "^2.0.9",
    "prisma": "^3.0.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
