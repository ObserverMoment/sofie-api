{
  "name": "sofie.fitness-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "heroku-postbuild": "if [ \"$NODE_ENV\" = \"sandbox\" ]; then npm run generate-build; else prisma migrate deploy && npm run generate-build; fi;",
    "build": "tsc",
    "generate-build": "npm run generate && npm run build",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon --delay 1 dist/index.js\"",
    "debug": "nodemon --inspect dist/index.js",
    "generate": "DEBUG=1 graphql-codegen --config codegen.yml",
    "studio": "prisma studio --port 5000",
    "dbpush": "prisma db push",
    "migrate": "prisma migrate dev",
    "resolve": "prisma migrate resolve --applied xxxxxx"
  },
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/merge": "^8.2.14",
    "@paljs/plugins": "^4.1.0",
    "@prisma/client": "^3.15.1",
    "apollo-server-core": "^3.8.2",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.8.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "firebase-admin": "^10.3.0",
    "getstream": "^8.0.1",
    "graphql": "^15.8.0",
    "graphql-config": "^4.3.1",
    "graphql-middleware": "^6.1.29",
    "graphql-parse-resolve-info": "^4.12.0",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "joi": "^17.6.0",
    "node-fetch": "^2.6.2",
    "stream-chat": "^6.6.0",
    "ws": "^8.8.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typescript": "2.5.1",
    "@graphql-codegen/typescript-document-nodes": "2.2.13",
    "@graphql-codegen/typescript-resolvers": "^2.6.6",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.43",
    "@types/node-fetch": "^2.6.1",
    "@types/uuid": "^8.3.4",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.16",
    "prisma": "^3.15.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
