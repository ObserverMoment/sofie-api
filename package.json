{
  "name": "spotme.fitness-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "debug": "nodemon --inspect dist/index.js",
    "generate": "graphql-codegen --config codegen.yml",
    "studio": "prisma studio --port 5000",
    "dbpush": "prisma db push --preview-feature",
    "postinstall": "npm run generate && prisma db push --preview-feature --accept-data-loss && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/merge": "^6.2.7",
    "@paljs/plugins": "^2.12.2",
    "@prisma/client": "^2.17.0",
    "apollo-server": "^2.21.0",
    "dotenv": "^8.2.0",
    "firebase-admin": "^9.5.0",
    "graphql-config": "^3.2.0",
    "graphql-middleware": "^6.0.4",
    "graphql-parse-resolve-info": "^4.11.0",
    "graphql-type-json": "^0.3.2",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.9",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@types/node": "^14.14.28",
    "@types/node-fetch": "^2.5.8",
    "concurrently": "^5.3.0",
    "graphql": "^15.5.0",
    "nodemon": "^2.0.7",
    "prisma": "^2.17.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
