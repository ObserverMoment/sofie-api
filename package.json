{
  "name": "spotme.fitness-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "debug": "nodemon --inspect dist/index.js",
    "generate": "graphql-codegen --config codegen.yml",
    "studio": "prisma studio --port 5000",
    "dbpush": "prisma db push",
    "postinstall": "prisma db push --accept-data-loss && npm run generate && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/merge": "^6.2.16",
    "@graphql-tools/schema": "^7.1.5",
    "@paljs/plugins": "^3.6.1",
    "@prisma/client": "^2.28.0",
    "@types/express": "^4.17.13",
    "apollo-server-express": "^3.1.1",
    "dotenv": "^10.0.0",
    "firebase-admin": "^9.11.0",
    "graphql-config": "^3.4.0",
    "graphql-middleware": "^6.0.10",
    "graphql-parse-resolve-info": "^4.12.0",
    "graphql-type-json": "^0.3.2",
    "joi": "^17.4.2",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.8",
    "@graphql-codegen/schema-ast": "^1.18.3",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.16",
    "@graphql-codegen/typescript-resolvers": "^1.20.0",
    "@types/node": "^16.4.10",
    "@types/node-fetch": "^2.5.12",
    "concurrently": "^6.2.0",
    "graphql": "^15.5.1",
    "nodemon": "^2.0.12",
    "prisma": "^2.28.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
