{
  "name": "spotme.fitness-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/index.js\"",
    "debug": "nodemon --inspect dist/index.js",
    "generate": "graphql-codegen --config codegen.yml",
    "studio": "prisma studio --port 5000",
    "dbpush": "prisma db push",
    "postinstall": "npm run generate && prisma db push --accept-data-loss && npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/merge": "^6.2.14",
    "@paljs/plugins": "^3.3.3",
    "@prisma/client": "^2.25.0",
    "@types/express": "^4.17.12",
    "apollo-server-express": "^2.24.1",
    "dotenv": "^10.0.0",
    "firebase-admin": "^9.8.0",
    "graphql-config": "^3.3.0",
    "graphql-middleware": "^6.0.10",
    "graphql-parse-resolve-info": "^4.11.0",
    "graphql-type-json": "^0.3.2",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/schema-ast": "^1.18.3",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-document-nodes": "1.17.12",
    "@graphql-codegen/typescript-resolvers": "^1.19.2",
    "@types/node": "^15.6.1",
    "@types/node-fetch": "^2.5.10",
    "concurrently": "^6.2.0",
    "graphql": "^15.5.0",
    "nodemon": "^2.0.7",
    "prisma": "^2.25.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
